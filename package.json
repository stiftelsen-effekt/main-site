{
  "name": "main-site",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "echo 'Building Sanity to public/studio' && npm i --prefix studio && cross-env SANITY_STUDIO_SITE_URL=$NEXT_PUBLIC_SITE_URL SANITY_STUDIO_VERCEL_ENV=$VERCEL_ENV SANITY_STUDIO_VERCEL_URL=$VERCEL_URL npm run build --prefix studio -- ../public/studio -y && echo 'Done'",
    "build": "next build",
    "sanity": "dotenv -e .env.local npm run start -- --prefix studio",
    "sanity:edit": "dotenv -e .env.local npm run edit -- --prefix studio",
    "sanity:list:pages": "dotenv -e .env.local npm run list:pages -- --prefix studio",
    "start": "next start",
    "lint": "next lint",
    "cypress": "cross-env CYPRESS_PATH=node_modules/.bin/cypress cypress open",
    "coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "format": "prettier --write \"./**/*{js,ts,jsx,tsx,json,md,css,scss,yml}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@babel/preset-typescript": "^7.16.7",
    "@observablehq/plot": "^0.6.8",
    "@portabletext/react": "^3.0.11",
    "@redux-devtools/extension": "^3.2.5",
    "@sanity/image-url": "^1.0.2",
    "@ssfbank/norwegian-id-validators": "^1.0.0",
    "@stdlib/stats-base-dists-truncated-normal": "^0.2.0",
    "auth0": "^3.2.0",
    "auth0-js": "^9.19.0",
    "babel-jest": "^27.5.1",
    "d3": "^7.8.2",
    "identity-obj-proxy": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "luxon": "^2.3.0",
    "moment": "^2.29.3",
    "next": "^12.1.6",
    "next-plausible": "^3.8.0",
    "next-sanity": "^1.1.0",
    "next-sanity-image": "^3.2.1",
    "next-seo": "^5.4.0",
    "organisationsnummer": "^1.1.5",
    "personnummer": "^3.2.1",
    "react": "17.0.2",
    "react-animate-height": "^3.1.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "17.0.2",
    "react-draggable": "^4.4.5",
    "react-feather": "^2.0.9",
    "react-hook-form": "^7.45.2",
    "react-hook-inview": "^4.5.0",
    "react-number-format": "^5.1.4",
    "react-redux": "^7.2.9",
    "react-test-renderer": "^17.0.2",
    "react-toastify": "^8.2.0",
    "react-youtube": "^10.1.0",
    "recharts": "^2.1.9",
    "redux-saga": "^1.1.3",
    "sass": "^1.53.0",
    "styled-components": "^5.3.5",
    "swr": "^1.2.2",
    "use-debounce": "^8.0.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@types/d3": "^7.1.0",
    "@types/d3-path": "^3.0.0",
    "@types/enzyme": "^3.10.11",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/istanbul-lib-coverage": "^2.0.4",
    "@types/jest": "^27.5.2",
    "@types/luxon": "^2.0.9",
    "@types/node": "18.15.11",
    "@types/react": "^17.0.39",
    "@types/styled-components": "^5.1.25",
    "@types/validator": "^13.7.3",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "cypress-dotenv": "^2.0.0",
    "cypress-localstorage-commands": "^1.7.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^13.2.1",
    "prettier": "2.8.1",
    "ts-jest": "^27.1.5",
    "typescript": "^4.5.5",
    "typescript-fsa": "^3.0.0",
    "webpack": "^5.70.0"
  },
  "overrides": {
    "@types/react": "^17.0.39"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,css,scss,yml}": "prettier --write"
  }
